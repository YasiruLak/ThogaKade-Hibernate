<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="root" prefHeight="643.0" prefWidth="979.0"
            stylesheets="@assests/style/Style..css" xmlns="http://javafx.com/javafx/8.0.171"
            fx:controller="lk.ijse.pos.controller.CustomerManageController">
    <children>
        <ImageView fitHeight="314.0" fitWidth="318.0" layoutX="14.0" layoutY="-107.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@assests/image/logo.png"/>
            </image>
        </ImageView>
        <TableView fx:id="tblCustomers" layoutX="15.0" layoutY="308.0" prefHeight="314.0" prefWidth="943.0">
            <columns>
                <TableColumn prefWidth="68.0" text="Id"/>
                <TableColumn prefWidth="79.0" text="Title"/>
                <TableColumn prefWidth="183.0" text="Name"/>
                <TableColumn prefWidth="222.0" text="Address"/>
                <TableColumn prefWidth="146.0" text="City"/>
                <TableColumn prefWidth="124.0" text="Province"/>
                <TableColumn prefWidth="119.0" text="Postal Code"/>
            </columns>
        </TableView>
        <ImageView id="imgHome" fitHeight="45.0" fitWidth="45.0" layoutX="14.0" layoutY="14.0"
                   onMouseClicked="#navigateToBack" pickOnBounds="true" preserveRatio="true">
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
            <image>
                <Image url="@assests/image/back_96px.png"/>
            </image>
        </ImageView>
        <Separator layoutX="332.0" layoutY="74.0" prefHeight="4.0" prefWidth="613.0"/>
        <Label alignment="CENTER" layoutX="331.0" layoutY="22.0" prefHeight="35.0" prefWidth="620.0"
               styleClass="liner-title" text="Customer Manage" textFill="WHITE">
            <font>
                <Font name="Times New Roman Bold" size="24.0"/>
            </font>
        </Label>
        <JFXTextField fx:id="txtCustomerId" labelFloat="true" layoutX="38.0" layoutY="151.0" prefHeight="28.0"
                      prefWidth="138.0" promptText="Customer Id :"/>
        <JFXTextField fx:id="txtCustomerName" labelFloat="true" layoutX="365.0" layoutY="151.0" prefHeight="28.0"
                      prefWidth="242.0" promptText="Name :"/>
        <JFXTextField fx:id="txtCustomerAddress" labelFloat="true" layoutX="38.0" layoutY="206.0" prefHeight="28.0"
                      prefWidth="304.0" promptText="Address :"/>
        <JFXComboBox fx:id="cmbCustomerProvince" layoutX="542.0" layoutY="206.0" prefHeight="28.0" prefWidth="150.0"
                     promptText="Province"/>
        <JFXTextField fx:id="txtCustomerCity" labelFloat="true" layoutX="360.0" layoutY="206.0" prefHeight="28.0"
                      prefWidth="159.0" promptText="City :"/>
        <JFXTextField fx:id="txtCustomerPostalCode" labelFloat="true" layoutX="716.0" layoutY="206.0" prefHeight="28.0"
                      prefWidth="150.0" promptText="Postal Code :"/>
        <JFXButton fx:id="btnAddNewCustomer" layoutX="794.0" layoutY="93.0" onAction="#btnAddNew_OnAction"
                   prefHeight="35.0" prefWidth="144.0" style="-fx-cursor: hand;" styleClass="btnAddNew"
                   stylesheets="@assests/style/Style..css" text="+ New Customer">
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </JFXButton>
        <JFXButton fx:id="btnSave" layoutX="768.0" layoutY="262.0" onAction="#btnSave_OnAction" prefHeight="35.0"
                   prefWidth="84.0" style="-fx-cursor: hand;" styleClass="btnSave" text="Save">
            <font>
                <Font size="14.0"/>
            </font>
        </JFXButton>
        <JFXButton id="btnDelete" fx:id="btnDelete" layoutX="861.0" layoutY="262.0" onAction="#btnDelete_OnAction"
                   prefHeight="35.0" prefWidth="84.0" style="-fx-cursor: hand;" styleClass="btnDelete" text="Delete">
            <font>
                <Font size="14.0"/>
            </font>
        </JFXButton>
        <JFXComboBox fx:id="cmbCustomerTitle" layoutX="204.0" layoutY="151.0" prefHeight="28.0" prefWidth="138.0"
                     promptText="Title"/>
    </children>
</AnchorPane>
